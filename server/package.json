{
  "name": "trackmanager-server",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <18.19.0"
  },
  "scripts": {
    "develop": "tsx watch --tsconfig ./tsconfig-build.json src/index.ts",
    "start": "tsx --tsconfig ./tsconfig-build.json src/index.ts",
    "build": "tsc --build --clean && tsc -p ./tsconfig-build.json",
    "typecheck": "tsc --noEmit -p ./tsconfig-build.json",
    "startbuild": "node ./dist/src/index.js",
    "test": "jest",
    "copyconfig": "tsx --tsconfig ./tsconfig-build.json ./src/bin/copyconfig.ts",
    "lint": "eslint --quiet .",
    "lintwarn": "eslint ."
  },
  "dependencies": {
    "@garmin/fitsdk": "^21.126.0",
    "@simplewebauthn/server": "^8.3.5",
    "connect-pg-simple": "^9.0.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "formidable": "^3.5.1",
    "geographiclib-geodesic": "^2.0.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@tsconfig/node18": "^18.2.2",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/formidable": "^3.4.2",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.3.4",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.4",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.2",
    "typescript-cp": "^0.1.9"
  }
}
